{
  "name": "@xstudio/publish",
  "version": "0.1.0",
  "private": true,
  "description": "XStudio publish runtime for deployed applications",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:ssg": "cross-env BUILD_MODE=ssg vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@xstudio/shared": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-aria-components": "catalog:"
  },
  "devDependencies": {
    "@xstudio/config": "workspace:*",
    "eslint": "^9.39.2",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react-swc": "catalog:",
    "cross-env": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  }
}
