  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:preview": "vite build --config vite.preview.config.ts",
    "build:all": "npm run build:preview && npm run build",
    "lint": "eslint src --ext .ts,.tsx",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "playwright:install": "npx playwright install",
    "test:perf:short": "npx tsx scripts/long-session-test.ts short",
    "test:perf:medium": "npx tsx scripts/long-session-test.ts medium",
    "test:perf:long": "npx tsx scripts/long-session-test.ts long",
    "analyze:perf-trends": "npx tsx scripts/track-metrics.ts",
    "check:browser:default": "node scripts/browser-check.js",
    "check:browser:quick": "node scripts/browser-check.js --quick",
    "check:browser:perf": "node scripts/browser-check.js",
    "test:e2e": "playwright test"
