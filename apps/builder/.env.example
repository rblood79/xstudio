# ============================================
# Feature Flags
# ============================================

# WebGL Canvas (CanvasKit/Skia + PixiJS) 활성화
# false 시 iframe 기반 Preview로 폴백
VITE_USE_WEBGL_CANVAS=true

# 디버그 로그 출력 (콘솔)
VITE_ENABLE_DEBUG_LOGS=false

# 캔버스 비교 모드 — iframe + WebGL 캔버스를 동시 표시하여 교차검증
VITE_CANVAS_COMPARE_MODE=false

# Fill V2 — 다중 Fill 레이어 + 색상 모드 전환 (Gradient, Image 등)
VITE_FEATURE_FILL_V2=false

# 히트 영역 디버그 시각화 — 투명 히트 영역을 빨간색 반투명으로 표시
VITE_ENABLE_HITAREA_MODE=false

# ============================================
# Supabase (Required)
# ============================================

VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ============================================
# AI Provider API Keys
# ============================================

# Groq — 빌더 내장 AI 어시스턴트 (llama-3.3-70b-versatile)
VITE_GROQ_API_KEY=your_groq_api_key_here

# Anthropic — Claude 모델
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Perplexity (Optional)
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# OpenAI / OpenRouter (Optional)
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini (Optional)
GOOGLE_API_KEY=your_google_api_key_here

# Mistral AI (Optional)
MISTRAL_API_KEY=your_mistral_api_key_here

# xAI (Optional)
XAI_API_KEY=your_xai_api_key_here

# Azure OpenAI (Optional, endpoint는 .taskmasterconfig에서 설정)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here

# ============================================
# External Services (Optional)
# ============================================

# Figma API — 테마/디자인 토큰 가져오기
VITE_FIGMA_ACCESS_TOKEN=your_figma_access_token_here
