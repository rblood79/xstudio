{
  "name": "@xstudio/shared",
  "version": "0.1.0",
  "private": true,
  "description": "XStudio shared types, utilities, components, and renderers",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    },
    "./components": {
      "types": "./src/components/index.tsx",
      "default": "./src/components/index.tsx"
    },
    "./components/*": {
      "types": "./src/components/*.tsx",
      "default": "./src/components/*.tsx"
    },
    "./components/list": {
      "types": "./src/components/list.ts",
      "default": "./src/components/list.ts"
    },
    "./components/metadata": {
      "types": "./src/components/metadata.ts",
      "default": "./src/components/metadata.ts"
    },
    "./components/styles/*": {
      "default": "./src/components/styles/*"
    },
    "./renderers": {
      "types": "./src/renderers/index.ts",
      "default": "./src/renderers/index.ts"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "default": "./src/hooks/index.ts"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint src/"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "dependencies": {
    "@internationalized/date": "^3.11.0",
    "@internationalized/number": "^3.6.5",
    "@react-aria/focus": "^3.21.4",
    "@react-aria/utils": "^3.33.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.19",
    "@types/node": "catalog:",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "react-aria-components": "catalog:",
    "react-stately": "^3.44.0",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@xstudio/config": "workspace:*",
    "eslint": "^9.39.2",
    "typescript": "catalog:",
    "zod": "^4.3.6"
  }
}
